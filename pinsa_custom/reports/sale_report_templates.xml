<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_saleorder_document_cust" inherit_id="sale.report_saleorder_document">
        <xpath expr="//table//tr//th[@name='th_quantity']" position="after">
            <th name="th_virtual_quantity" class="text-right">Cajas</th>
        </xpath>
        <xpath expr="//td[@name='td_quantity']" position="after">
            <td name="td_quantity" class="text-right">
                <span t-field="line.qty_box"/>
            </td>
        </xpath>
    </template>
</odoo>
