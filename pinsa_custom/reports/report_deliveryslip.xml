<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_delivery_document_cust" inherit_id="stock.report_delivery_document">
        <xpath expr="//table[@name='stock_move_table']//th[@name='th_sm_product']" position="after">
            <th name="th_virtual_quantity">CAJAS</th>
        </xpath>
        <xpath expr="//span[@t-field='move.product_id']/.." position="after">
            <td>
                <span t-field="move.qty_box"/>
            </td>
        </xpath>
        <xpath expr="//table[@name='stock_move_line_table']//th[@name='th_sml_product']" position="after">
            <th name="th_virtual_quantity" class="text-center"><strong>CAJAS</strong></th>
        </xpath>
        <xpath expr="//span[@t-field='move_line.product_id']/.." position="after">
            <td class="text-center">
                <span t-field="move_line.move_id.qty_box"/>
            </td>
        </xpath>    
    </template>
</odoo>
